{
  "private": true,
  "name": "restroom-mw",
  "repository": "https://github.com/dyne/restroom-mw",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": "^14.15.0 || ^16.13.0"
  },
  "scripts": {
    "prerelease": "yarn build && yarn doc && git add docs && git commit -m 'docs: ğŸ“šï¸ ğŸ“ Update docs'",
    "release": "npm whoami && lerna publish",
    "release:next": "lerna publish --canary -y --pre-dist-tag next --preid $(git rev-parse --short HEAD)",
    "c": "gacp --emoji emoji --add false",
    "clean": "turbo run clean",
    "unlinks": "turbo run unlinks",
    "links": "turbo run links",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "pretest": "turbo run build",
    "test": "c8 ava",
    "watch:test": "ava -s --watch",
    "watch": "turbo run watch --parallel",
    "build": "turbo run build --parallel",
    "serve": "babel-watch test/fixtures/server.js",
    "doc": "turbo run doc"
  },
  "turbo": {
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ]
      },
      "watch": {
        "outputs": []
      },
      "doc": {
        "outputs": [
          "docs/**"
        ]
      },
      "lint": {
        "outputs": []
      },
      "lint:fix": {
        "outputs": []
      },
      "clean": {
        "outputs": []
      },
      "links": {
        "outputs": []
      },
      "unlinks": {
        "outputs": []
      }
    }
  },
  "ava": {
    "failFast": true,
    "verbose": true,
    "timeout": "1m",
    "extensions": [
      "ts",
      "js"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "reporter": [
      "lcovonly",
      "text"
    ],
    "lines": 85,
    "check-coverage": true
  },
  "husky": {
    "hooks": {
      "commit-msg": "devmoji -e",
      "pre-commit": "yarn test"
    }
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.12.16",
    "@babel/highlight": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/register": "^7.12.13",
    "@types/body-parser": "^1.19.0",
    "@types/cbor": "^5.0.1",
    "@types/express": "^4.17.11",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.1.3",
    "ava": "^4.0.1",
    "babel-watch": "^7.4.0",
    "c8": "^7.6.0",
    "cbor": "^7.0.1",
    "cypress": "^6.5.0",
    "devmoji": "^2.1.13",
    "documentation": "^13.1.1",
    "errorhandler": "^1.5.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "gacp": "^2.10.2",
    "husky": "^5.0.9",
    "lerna": "^3.22.1",
    "morgan": "^1.10.0",
    "prettier": "^2.2.1",
    "qs": "^6.9.6",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4",
    "start-server-and-test": "^1.12.0",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "turbo": "^1.0.9",
    "typescript": "^4.1.5",
    "zenroom": "^2.2.0"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.12.16",
    "@dyne/sawroom-client": "^1.9.0"
  }
}
