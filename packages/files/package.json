{
  "name": "@restroom-mw/files",
  "version": "0.11.0",
  "description": "Utilities middleware to work with files for Restroom",
  "author": "Alberto Lerda <alberto@dyne.org>",
  "contributors": [
    "Puria Nafisi Azizi <puria@dyne.org>"
  ],
  "homepage": "https://dyne.github.io/restroom-mw/",
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyne/restroom-mw.git"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "doc": "documentation readme src/** -g -f md -s 'API' --readme-file ../../docs/packages/files.md --shallow",
    "links": "yarn link",
    "unlinks": "yarn unlink",
    "watch": "tsc -b tsconfig.json -w",
    "build": "tsc -b tsconfig.json"
  },
  "bugs": {
    "url": "https://github.com/dyne/restroom-mw/issues"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "extract-zip" :"2.0.1"
  }
}
